<%= turbo_frame_tag dom_id(product) do %>
  <tr>
    <td><%= product.name %></td>
    <td><%= link_to 'Show', kit_product_path(product) %></td>
    <td><%= link_to 'Edit', edit_kit_product_path(product) %></td>
    <td><%= link_to 'Destroy', kit_product_path(product), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>